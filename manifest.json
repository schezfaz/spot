{
    "manifest_version" : 2,
    "name" : "SPOT",
    "version": "1.0",
    "description": "An extension that allows you add a song playing on Youtube to any of your playlists on spotify.",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },

    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"    
    }
}